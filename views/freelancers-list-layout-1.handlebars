</head>
<body class="gray">

<!-- Wrapper -->
<div id="wrapper">

<!-- Header Container
================================================== -->
<header id="header-container" class="fullwidth">

	<!-- Header -->
	<div id="header">
		<div class="container">
			
			<!-- Left Side Content -->
			<div class="left-side">
				
				<!-- Logo -->
				<div id="logo">
					<a href="/"><img src="/images/logo.png" alt=""></a>
				</div>

				<!-- Main Navigation -->
				<nav id="navigation">
					<ul id="responsive">

						<li><a href="/">خانه</a></li>

						<li><a href="#">فرصت‌های کاری</a>
							<ul class="dropdown-nav">
								<li><a href="/jobs">شغل‌ها</a>
								</li>
								<li><a href="/tasks">پروژه‌ها</a>
									
								</li>
							</ul>
						</li>

						<li><a href="/companies">شرکت‌ها</a></li>

						<li><a href="/freelancers" class="current">فریلنسرها</a></li>
					</ul>
				</nav>
				<div class="clearfix"></div>
				<!-- Main Navigation / End -->
				
			</div>
			<!-- Left Side Content / End -->


			<!-- Right Side Content / End -->
			<div class="right-side">
				{{#if data.user}}
				{{#equal data.user.role 'freelancer'}}
				<!-- User Menu -->
				<div class="header-widget">

					<!-- Messages -->
					<div class="header-notifications user-menu">
						<div class="header-notifications-trigger">
							{{#if data.user.profile_picture}}
							<a href="#"><div class="user-avatar status-online"><img src="{{data.user.profile_picture}}" alt=""></div></a>
							{{else}}
							<a href="#"><div class="user-avatar status-online"><img src="/images/user-avatar-placeholder.png" alt=""></div></a>
							{{/if}}
						</div>

						<!-- Dropdown -->
						<div class="header-notifications-dropdown">

							<!-- User Status -->
							<div class="user-status">

								<!-- User Name / Avatar -->
								<div class="user-details">
									{{#if data.user.profile_picture}}
									<div class="user-avatar status-online"><img src="{{data.user.profile_picture}}" alt=""></div>
									{{else}}
									<div class="user-avatar status-online"><img src="/images/user-avatar-placeholder.png" alt=""></div>
									{{/if}}
									<div class="user-name">
										{{data.user.first_name}} {{data.user.last_name}} <span>Freelancer</span>
									</div>
								</div>
								
								<!-- User Status Switcher -->
								<div class="status-switch" id="snackbar-user-status">
									<label class="user-online current-status">Online</label>
									<label class="user-invisible">Invisible</label>
									<!-- Status Indicator -->
									<span class="status-indicator" aria-hidden="true"></span>
								</div>	
							</div>
							
							<ul class="user-menu-small-nav">
								<li><a href="/dashboard"><i class="icon-material-outline-dashboard"></i> داشبورد</a></li>
								<li><a href="/dashboard_settings"><i class="icon-material-outline-settings"></i> تنطیمات</a></li>
								<li><a href="/logout" id="logOut"><i class="icon-material-outline-power-settings-new"></i> خروج</a></li>
							</ul>
						</div>
					</div>
				</div>
				<!-- User Menu / End -->
				{{/equal}}
				{{#equal data.user.role 'employer'}}
				<!-- User Menu -->
				<div class="header-widget">

					<!-- Messages -->
					<div class="header-notifications user-menu">
						<div class="header-notifications-trigger">
							{{#if data.user.logo}}
							<a href="#"><div class="user-avatar status-online"><img src="{{data.user.logo}}" alt=""></div></a>
							{{else}}
							<a href="#"><div class="user-avatar status-online"><img src="/images/user-avatar-placeholder.png" alt=""></div></a>
							{{/if}}
						</div>

						<!-- Dropdown -->
						<div class="header-notifications-dropdown">

							<!-- User Status -->
							<div class="user-status">

								<!-- User Name / Avatar -->
								<div class="user-details">
									{{#if data.user.logo}}
									<div class="user-avatar status-online"><img src="{{data.user.logo}}" alt=""></div>
									{{else}}
									<div class="user-avatar status-online"><img src="/images/user-avatar-placeholder.png" alt=""></div>
									{{/if}}
									<div class="user-name">
										{{data.user.name}} <span>Employer</span>
									</div>
								</div>
								
								<!-- User Status Switcher -->
								<div class="status-switch" id="snackbar-user-status">
									<label class="user-online current-status">Online</label>
									<label class="user-invisible">Invisible</label>
									<!-- Status Indicator -->
									<span class="status-indicator" aria-hidden="true"></span>
								</div>	
							</div>
							
							<ul class="user-menu-small-nav">
								<li><a href="/dashboard"><i class="icon-material-outline-dashboard"></i> داشبورد</a></li>
								<li><a href="/dashboard_settings"><i class="icon-material-outline-settings"></i> تنطیمات</a></li>
								<li><a href="/logout" id="logOut"><i class="icon-material-outline-power-settings-new"></i> خروج</a></li>
							</ul>
						</div>
					</div>
				</div>
				<!-- User Menu / End -->
				{{/equal}}
				{{/if}}
				<!-- Mobile Navigation Button -->
				<span class="mmenu-trigger">
					<button class="hamburger hamburger--collapse" type="button">
						<span class="hamburger-box">
							<span class="hamburger-inner"></span>
						</span>
					</button>
				</span>

			</div>
			<!-- Right Side Content / End -->

		</div>
	</div>
	<!-- Header / End -->

</header>
<div class="clearfix"></div>
<!-- Header Container / End -->

<!-- Spacer -->
<div class="margin-top-90"></div>
<!-- Spacer / End-->

<!-- Page Content
================================================== -->
<div class="container">
	<div class="row">
		<div class="col-xl-3 col-lg-4">
			<div class="sidebar-container">
				
				<!-- Location -->
				<div class="sidebar-widget">
					<h3>موقعیت</h3>
					<div class="input-with-icon">
						<div id="autocomplete-container">
							<input id="autocomplete-input" type="text" placeholder="موقعیت">
						</div>
						<i class="icon-material-outline-location-on"></i>
					</div>
				</div>

				<!-- Keywords -->
				<div class="sidebar-widget">
					<h3>عنوان</h3>
					<div class="keywords-container">
						<div class="keyword-input-container">
							<input id="tagline" type="text" class="keyword-input" placeholder="Web Developer"/>
						</div>
						<div class="keywords-list"><!-- keywords go here --></div>
						<div class="clearfix"></div>
					</div>
				</div>

				<div class="sidebar-widget">

					<button class="button ripple-effect full-width" id="submit">فیلتر کن</button>

				</div>

				<div class="sidebar-widget">
					
					<button class="button ripple-effect full-width" style="background-color: #303030;" id="clear">پاک کن</button>
					<div class="clearfix"></div>

				</div>

			</div>
		</div>
		<div class="col-xl-9 col-lg-8 content-left-offset">
			
			<!-- Freelancers List Container -->
			<div class="freelancers-container freelancers-list-layout compact-list margin-top-35">
				
				{{#each data.freelancers}}
				<!--Freelancer -->
				<div class="freelancer">

					<!-- Overview -->
					<div class="freelancer-overview">
						<div class="freelancer-overview-inner">
							
							<!-- Bookmark Icon -->
							<span class="bookmark-icon"></span>
							
							<!-- Avatar -->
							<div class="freelancer-avatar">
								<a href="/freelancers/{{this._id}}"><img src="{{this.profile_picture}}" alt=""></a>
							</div>

							<!-- Name -->
							<div class="freelancer-name">
								<h4 style="font-family: Mahsa;"><a href="#">{{this.first_name}} {{this.last_name}} </a></h4>
								<span style="font-family: Mahsa;">{{this.tag_line}}</span>
								<!-- Rating -->
								<div class="freelancer-rating" style="font-family: Mahsa;">
									<div class="star-rating" data-rating="{{this.rate}}"></div>
								</div>
							</div>
						</div>
					</div>
					
					<!-- Details -->
					<div class="freelancer-details" style="font-family: Mahsa;">
						<div class="freelancer-details-list">
							<ul>
								<li>موقعیت <strong><i class="icon-material-outline-location-on"></i> {{this.nationality}}
								</strong></li>
								<li>امتباز <strong>{{this.minimal_hourly_rate}} / hr</strong></li>
								<li></li>
							</ul>
						</div>
						<a href="/freelancers/{{this._id}}" class="button button-sliding-icon ripple-effect">مشاهده پروفایل <i class="icon-material-outline-arrow-right-alt"></i></a>
					</div>
				</div>
				{{/each}}
				<!-- Freelancer / End -->
			</div>

			<!-- Pagination -->
			<div class="clearfix"></div>
			<div class="row">
				<div class="col-md-12">
					<!-- Pagination -->
					<div class="pagination-container margin-top-40 margin-bottom-60">
						<nav class="pagination">
							<ul class="page-no">
								<li><a href="/freelancers?page=1" class="ripple-effect">1</a></li>
								<li><a href="/freelancers?page=2" class="ripple-effect">2</a></li>
								<li><a href="/freelancers?page=3" class="ripple-effect">3</a></li>
							</ul>
						</nav>
					</div>
				</div>
			</div>
			<!-- Pagination / End -->

		</div>
	</div>
</div>


