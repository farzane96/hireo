		    }, 300);
		}
	}
</script>

<script>
	$(document).ready(() => {
		$('#nationality option').each((index, element) => {
			if ($('#lc').text() === $(element).val()) {
				$(element).attr('selected', 'selected');
			}
		});

		$('#save_changes').on('click', function() {
			const first_name = $('#first_name').val();
			const last_name = $('#last_name').val();
			let skills = [];
			$('.keywords-list').children().each(function() {
				skills.push(this.childNodes[1].textContent);
			});
			const rate = $('#rate').val();
			const tag_line = $('#tag_line').val();
			const nationality = $('#nationality option:selected').text();
			const bio = $('#bio').val();
			$.ajax({
				url: '/dashboard_settings',
				method: 'POST',
				contentType: 'application/json', 
				data: JSON.stringify({ 
					first_name, 
					last_name, 
					skills, 
					rate, 
					tag_line, 
					nationality, 
					bio 
				}),
				success: (data, status, xhr) => {
					Snackbar.show({
						text: 'Your profile has been changed!',
						pos: 'bottom-center',
						showAction: false,
						actionText: "Dismiss",
						duration: 3000,
						textColor: '#fff',
						backgroundColor: '#383838'
					});
				},
			});
		});

		$('#save_employer').on('click', function() {
			const name = $('#emp_name').val();
			const location = $('#location option:selected').text();
			const bio = $('#bio').val();
			$.ajax({
				url: '/dashboard_settings',
				method: 'POST',
				contentType: 'application/json', 
				data: JSON.stringify({ 
					name,
					location,
					bio,
				}),
				success: (data, status, xhr) => {
					Snackbar.show({
						text: 'Your profile has been changed!',
						pos: 'bottom-center',
						showAction: false,
						actionText: "Dismiss",
						duration: 3000,
						textColor: '#fff',
						backgroundColor: '#383838'
					});
				},
			});
		});
	});

	$('#logOut').on('click', () => {
		$.ajax({
			url: '/logout',
			method: 'GET',
			success: (data, status, xhr) => {
				window.location.href = '/';
			}
		});
	});

	$('#location option').each((index, element) => {
		if ($('#lc').text() === $(element).text()) {
			$(element).attr('selected', 'selected');
		}
	});
</script>

