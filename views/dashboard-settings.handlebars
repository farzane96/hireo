												<span class="bidding-detail">حداقل نرخ خود برای انجام پروژه‌ وارد کنید</span>

												<!-- Slider -->
												<div class="bidding-value margin-bottom-10"><span id="biddingVal"></span></div>
												<input class="bidding-slider" type="text" id="rate" value="{{data.minimal_hourly_rate}}" data-slider-handle="custom" data-slider-currency="$" data-slider-min="5" data-slider-max="150" data-slider-value="{{data.minimal_hourly_rate}}" data-slider-step="1" data-slider-tooltip="hide" />
											</div>
										</div>
									</div>

									<div class="col-xl-8">
										<div class="submit-field">
											<h5>مهارت‌ها <i class="help-icon" data-tippy-placement="right" title="Add up to 10 skills"></i></h5>

											<!-- Skills List -->
											<div class="keywords-container">
												<div class="keyword-input-container">
													<input type="text" class="keyword-input with-border" placeholder="e.g. Angular, Laravel"/>
													<button class="keyword-input-button ripple-effect"><i class="icon-material-outline-add"></i></button>
												</div>
												<div class="keywords-list">
													{{#each data.skills}}
													<span class="keyword"><span class="keyword-remove"></span><span class="keyword-text">{{this}}</span></span>
													{{/each}}
												</div>
												<div class="clearfix"></div>
											</div>
										</div>
									</div>

								</div>
							</li>
							<li>
								<div class="row">
									<div class="col-xl-6">
										<div class="submit-field">
											<h5>Tagline</h5>
											<input type="text" class="with-border" id="tag_line" value="{{data.tag_line}}">
										</div>
									</div>

									<div class="col-xl-6">
										<div class="submit-field">
											<h5>ملیت</h5>
											<select class="selectpicker with-border" data-size="7" id="nationality" title="Select Job Type" data-live-search="true">
												{{#each countries}}
												<option>{{this}}</option>
												{{#equal this data.nationality}}
												<option selected>{{this}}</option>
												{{/equal}}
												{{/each}}
											</select>
										</div>
									</div>

									<div class="col-xl-12">
										<div class="submit-field">
											<h5>خودتان رت معرفی کنید</h5>
											<textarea cols="30" rows="5" id="bio" class="with-border">{{data.bio}}</textarea>
										</div>
									</div>

								</div>
							</li>
						</ul>
						</div>
					</div>
				</div>

				<!-- Button -->
				<div class="col-xl-12">
					<a href="#" class="button ripple-effect big margin-top-30" id="save_changes">ذخیره تغییرات</a>
				</div>

			</div>
			<!-- Row / End -->

			<!-- Footer -->
			<div class="dashboard-footer-spacer"></div>
			<div class="small-footer margin-top-15">
				<div class="small-footer-copyrights">
					© 2020 <strong>Hireo</strong>. All Rights Reserved.
				</div>
				<ul class="footer-social-links">
					<li>
						<a href="#" title="Facebook" data-tippy-placement="top">
							<i class="icon-brand-facebook-f"></i>
						</a>
					</li>
					<li>
						<a href="#" title="Twitter" data-tippy-placement="top">
							<i class="icon-brand-twitter"></i>
						</a>
					</li>
					<li>
						<a href="#" title="Google Plus" data-tippy-placement="top">
							<i class="icon-brand-google-plus-g"></i>
						</a>
					</li>
					<li>
						<a href="#" title="LinkedIn" data-tippy-placement="top">
							<i class="icon-brand-linkedin-in"></i>
						</a>
					</li>
				</ul>
				<div class="clearfix"></div>
			</div>
			<!-- Footer / End -->

		</div>
	</div>
	<h5 id="lc" style="display: none;">{{user.nationality}}</h5>
	<!-- Dashboard Content / End -->
	{{/equal}}

