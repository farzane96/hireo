<!doctype html>
<html lang="en">

<!-- Mirrored from www.vasterad.com/themes/hireo_082019/dashboard-manage-tasks.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 29 Jan 2020 11:18:50 GMT -->
<head>

<!-- Basic Page Needs
================================================== -->
<title>Hireo</title>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

<!-- CSS
================================================== -->
<link rel="stylesheet" href="/css/style.css">
<link rel="stylesheet" href="/css/colors/blue.css">
<style>
    @font-face {
        font-family: 'Mahsa';
        src: url('/fonts/Mahsa.eot?#') format('eot'),  
            url('/fonts/Mahsa.woff') format('woff'),
            url('/fonts/Mahsa.ttf') format('truetype');
        font-style:normal;
        font-weight:normal;
    }
    body {
        font-family: Mahsa;
    }
</style>
</head>
<body class="gray">

<!-- Wrapper -->
<div id="wrapper">

<!-- Wrapper -->
<!-- Header Container
================================================== -->
<header id="header-container" class="fullwidth dashboard-header not-sticky">

	<!-- Header -->
	<div id="header">
        <div id="logo" style="left: 0; position: absolute;">
			<a href="/"><img src="/images/logo.png" alt=""></a>
		</div>
		<div class="container">
			<!-- Left Side Content -->
			<div class="right-side" style="position: relative; right: 150px;">
				<!-- Main Navigation -->
				<nav id="navigation">
					<ul id="responsive">
						<li><a>فرصت‌های کاری</a>
							<ul class="dropdown-nav">
								<li><a href="/jobs">شغل‌ها</a>
								</li>
								<li><a href="/tasks">پروژه‌ها</a>
								</li>
							</ul>
						</li>
						<li><a href="/companies">شرکت‌ها</a></li>
						<li><a href="/freelancers">فریلنسرها</a></li>
						<li><a href="/">خانه</a></li>
					</ul>
				</nav>
				<div class="clearfix"></div>
				<!-- Main Navigation / End -->
				
			</div>
			<!-- Left Side Content / End -->


			<!-- Right Side Content / End -->
			<div class="right-side">
				{{#if data.user}}
				{{#equal data.user.role 'freelancer'}}
				<!-- User Menu -->
				<div class="header-widget">

					<!-- Messages -->
					<div class="header-notifications user-menu">
						<div class="header-notifications-trigger">
							{{#if data.user.profile_picture}}
							<a href="#"><div class="user-avatar status-online"><img src="{{data.user.profile_picture}}" alt=""></div></a>
							{{else}}
							<a href="#"><div class="user-avatar status-online"><img src="/images/user-avatar-placeholder.png" alt=""></div></a>
							{{/if}}
						</div>

						<!-- Dropdown -->
						<div class="header-notifications-dropdown">

							<!-- User Status -->
							<div class="user-status">

								<!-- User Name / Avatar -->
								<div class="user-details">
									{{#if data.user.profile_picture}}
									<div class="user-avatar status-online"><img src="{{data.user.profile_picture}}" alt=""></div>
									{{else}}
									<div class="user-avatar status-online"><img src="/images/user-avatar-placeholder.png" alt=""></div>
									{{/if}}
									<div class="user-name">
										{{data.user.first_name}} {{data.user.last_name}} <span>Freelancer</span>
									</div>
								</div>
								
								<!-- User Status Switcher -->
								<div class="status-switch" id="snackbar-user-status">
									<label class="user-online current-status">Online</label>
									<label class="user-invisible">Invisible</label>
									<!-- Status Indicator -->
									<span class="status-indicator" aria-hidden="true"></span>
								</div>	
							</div>
							
							<ul class="user-menu-small-nav">
								<li><a href="/dashboard"><i class="icon-material-outline-dashboard"></i> داشبورد</a></li>
								<li><a href="/dashboard_settings"><i class="icon-material-outline-settings"></i> تنظیمات</a></li>
								<li><a href="/logout" id="logOut"><i class="icon-material-outline-power-settings-new"></i> خروج</a></li>
							</ul>
						</div>
					</div>
				</div>
				<!-- User Menu / End -->
				{{/equal}}
				{{#equal data.user.role 'employer'}}
				<!-- User Menu -->
				<div class="header-widget">

					<!-- Messages -->
					<div class="header-notifications user-menu">
						<div class="header-notifications-trigger">
							{{#if data.user.logo}}
							<a href="#"><div class="user-avatar status-online"><img src="{{data.user.logo}}" alt=""></div></a>
							{{else}}
							<a href="#"><div class="user-avatar status-online"><img src="/images/user-avatar-placeholder.png" alt=""></div></a>
							{{/if}}
						</div>

						<!-- Dropdown -->
						<div class="header-notifications-dropdown">

							<!-- User Status -->
							<div class="user-status">

								<!-- User Name / Avatar -->
								<div class="user-details">
									{{#if data.user.logo}}
									<div class="user-avatar status-online"><img src="{{data.user.logo}}" alt=""></div>
									{{else}}
									<div class="user-avatar status-online"><img src="/images/user-avatar-placeholder.png" alt=""></div>
									{{/if}}
									<div class="user-name">
										{{data.user.name}} <span>Employer</span>
									</div>
								</div>
								
								<!-- User Status Switcher -->
								<div class="status-switch" id="snackbar-user-status">
									<label class="user-online current-status">Online</label>
									<label class="user-invisible">Invisible</label>
									<!-- Status Indicator -->
									<span class="status-indicator" aria-hidden="true"></span>
								</div>	
							</div>
							
							<ul class="user-menu-small-nav">
								<li><a href="/dashboard"><i class="icon-material-outline-dashboard"></i> داشبورد</a></li>
								<li><a href="/dashboard_settings"><i class="icon-material-outline-settings"></i> تنظیمات</a></li>
								<li><a href="/logout" id="logOut"><i class="icon-material-outline-power-settings-new"></i> خروج</a></li>
							</ul>
						</div>
					</div>
				</div>
				<!-- User Menu / End -->
				{{/equal}}
				{{/if}}
				<!-- Mobile Navigation Button -->
				<span class="mmenu-trigger">
					<button class="hamburger hamburger--collapse" type="button">
						<span class="hamburger-box">
							<span class="hamburger-inner"></span>
						</span>
					</button>
				</span>

			</div>
			<!-- Right Side Content / End -->

		</div>
	</div>
	<!-- Header / End -->

</header>
<div class="clearfix"></div>
<!-- Header Container / End -->


<!-- Dashboard Container -->
<div class="dashboard-container">

	<!-- Dashboard Sidebar
	================================================== -->
	<div class="dashboard-sidebar">
		<div class="dashboard-sidebar-inner" data-simplebar>
			<div class="dashboard-nav-container">

				<!-- Responsive Navigation Trigger -->
				<a href="#" class="dashboard-responsive-nav-trigger">
					<span class="hamburger hamburger--collapse" >
						<span class="hamburger-box">
							<span class="hamburger-inner"></span>
						</span>
					</span>
					<span class="trigger-title">Dashboard Navigation</span>
				</a>
				
				<!-- Navigation -->
				<div class="dashboard-nav">
					<div class="dashboard-nav-inner">

						<ul data-submenu-title="شروع">
							<li><a href="/dashboard" class="current"><i class="icon-material-outline-dashboard"></i> داشبورد</a></li>
							<li><a href="/reviews/my_reviews"><i class="icon-material-outline-rate-review"></i> نظرات</a></li>
						</ul>
						
						{{#equal data.user.role 'employer'}}
						<ul data-submenu-title="سازمان دادن و اداره کردن">
							<li><a href="#"><i class="icon-material-outline-business-center"></i> شغل‌ها</a>
								<ul class="dropdown-nav">
									<li><a href="/jobs/my_jobs">مدیریت شغل‌ها <span class="nav-tag">{{data.user.jobs.length}}</span></a></li>
									<li><a href="/jobs/create">پست موقعیت شغلی</a></li>
								</ul>	
							</li>
							<li class="active-submenu"><a href="#"><i class="icon-material-outline-assignment"></i> پروژه‌ها</a>
								<ul class="dropdown-nav">
									<li><a href="/tasks/my_tasks">مدیریت پروژه‌ها <span class="nav-tag">{{data.user.tasks.length}}</span></a></li>
									<li><a href="/tasks/create">ساخت پروژه</a></li>
								</ul>	
							</li>
						</ul>
						{{/equal}}

						{{#equal data.user.role 'freelancer'}}
						<ul data-submenu-title="سازمان دادن و اداره کردن">
							<li class="active-submenu"><a href="#"><i class="icon-material-outline-assignment"></i> پروژه‌ها</a>
								<ul class="dropdown-nav">
									<li><a href="/tasks/my_tasks">مدیریت پروژه‌ها <span class="nav-tag">{{data.user.tasks.length}}
									</span></a></li>
									<li><a href="/tasks/my_bids">پیشنهادهای من </a></li>
								</ul>	
							</li>
						</ul>
						{{/equal}}

						<ul data-submenu-title="اکانت">
							<li><a href="/dashboard_settings"><i class="icon-material-outline-settings"></i> تنظیمات</a></li>
							<li><a href="/logout" id="logOut"><i class="icon-material-outline-power-settings-new"></i> خروج</a></li>
						</ul>
						
					</div>
				</div>
				<!-- Navigation / End -->

			</div>
		</div>
	</div>
	<!-- Dashboard Sidebar / End -->


	<!-- Dashboard Content
	================================================== -->
	<div class="dashboard-content-container" data-simplebar>
		<div class="dashboard-content-inner" >
			<!-- Dashboard Headline -->
			<div class="dashboard-headline">
				<h3 style="font-family: Mahsa; text-align: right">مدیریت پروژه‌ها</h3>
			</div>
			<!-- Row -->
			<div class="row" dir="rtl">
				<!-- Dashboard Box -->
				<div class="col-xl-12">
					<div class="dashboard-box margin-top-0">
						<!-- Headline -->
						<div class="headline">
							<h3 style="text-align: right; font-family: Mahsa"><i class="icon-material-outline-assignment"></i> پروژه‌های من</h3>
						</div>
						<div class="content">
							<ul class="dashboard-box-list" style="text-align: right;">
								{{#each data.user.tasks}}
								<li>
									<!-- Job Listing -->
									<div class="job-listing" style="width: 100%;">

										<!-- Job Listing Details -->
										<div class="job-listing-details">

											<!-- Details -->
											<div class="job-listing-description">
												<h3 class="job-listing-title"><a style="font-family: Mahsa" href="#">{{ this.name }}</a>
												{{#equal this.status 1}}
												    <span class="dashboard-status-button yellow" style="font-family: Mahsa">درحال بررسی</span></h3>
												{{/equal}}
												{{#equal this.status 2}}
												    <span class="dashboard-status-button" style="font-family: Mahsa; background-color: #66cbf3; color: #2664e9;">درحال انجام</span></h3>
												{{/equal}}
                                                {{#equal this.status 3}}
												    <span class="dashboard-status-button green" style="font-family: Mahsa">انجام شده</span></h3>
												{{/equal}}
                                                {{#equal this.status 4}}
												    <span class="dashboard-status-button red" style="font-family: Mahsa">منقضی شده</span></h3>
												{{/equal}}
                                                <!-- Task Description For Freelancer -->
                                                {{#equal ../data.user.role 'freelancer'}}
                                                    <h3 style="width: 100%; font-family: Mahsa">{{ trim this.description }}...</h3>
                                                {{/equal}}
                                                <!-- End /// Task Description For Freelancer -->
                                                <!-- Task Description For Employer -->
                                                {{#equal ../data.user.role 'employer'}}
                                                    {{#ne this.status 1}}
                                                        <h3 style="width: 100%; font-family: Mahsa">{{ trim this.description }}...</h3>
                                                    {{/ne}}
                                                {{/equal}}
                                                <!-- End /// Task Description For Employer -->
												<!-- Job Listing Footer -->
												<div class="job-listing-footer">
													<ul>
														<li style="font-family: Mahsa">{{this.created_at}}</li>
													</ul>
												</div>
											</div>
                                            <!-- Task Bids For Employer -->
                                            {{#equal ../data.user.role 'employer'}}
                                                {{#equal this.status 1}}
                                                    <ul class="dashboard-task-info" style="left: 0; right: auto;" dir="ltr">
                                                        {{#unless this.bids.length}}
                                                            <li><strong>0</strong><span>تعداد پیشنهادها</span></li>
                                                        {{/unless}}
                                                        {{#if this.bids.length}}
                                                            <li><strong>{{ this.bids.length }}</strong><span>تعداد پیشنهادها</span></li>
                                                        {{/if}}
                                                        <li><strong>{{this.bid_avg}}</strong><span>میانگین قیمت‌های پیشنهادی</span></li>
                                                        <li><strong>{{this.min_budget}} - {{this.max_budget}}</strong>
                                                        <span>قیمت {{this.budget_type}}</span>
                                                        </li>
                                                    </ul>
                                                {{/equal}}
                                            {{/equal}}
                                            <!-- End /// Task Bids For Employer -->
										</div>
									</div>
									
									
									<!-- Bidders Button For Employer -->
                                    {{#equal ../data.user.role 'employer'}}
                                        {{#equal this.status 1}}
                                            <div class="buttons-to-right always-visible">
                                                <a href="/tasks/{{this._id}}/bidders" class="button ripple-effect">مدیریت پیشنهادها
                                                    {{#unless this.bids.length}}
                                                        <span class="button-info" style="margin: 0 8px;">0</span>
                                                    {{/unless}}
                                                    {{#if this.bids.length}}
                                                        <span class="button-info" style="margin: 0 8px;">{{ this.bids.length }}</span>
                                                    {{/if}}
                                                </a>
                                            </div>
                                        {{/equal}}
                                    {{/equal}}
                                    <!-- End /// Bidders Button For Employer -->
                                    <!-- Details Button For Employer -->
                                    {{#equal ../data.user.role 'employer'}}
                                        {{#ne this.status 1}}
                                            <div class="buttons-to-right always-visible">
                                                <a href="/tasks/{{this._id}}" class="button ripple-effect">جزییات</a>
                                            </div>
                                        {{/ne}}
                                    {{/equal}}
                                     <!-- End /// Details Button For Employer -->
                                    <!-- Details Button For Freelancer -->
                                    {{#equal ../data.user.role 'freelancer'}}
                                        <div class="buttons-to-right always-visible">
                                            <a href="/tasks/{{this._id}}" class="button ripple-effect">جزییات</a>
                                        </div>
                                    {{/equal}}
                                     <!-- End /// Details Button For Freelancer -->
								</li>
								{{/each}}
							</ul>
						</div>
					</div>
				</div>

			</div>
			<!-- Row / End -->

			<!-- Footer -->
			<div class="dashboard-footer-spacer"></div>
			<div class="small-footer margin-top-15">
				<div class="small-footer-copyrights">
					© 2020 <strong>Hireo</strong>. All Rights Reserved.
				</div>
				<ul class="footer-social-links">
					<li>
						<a href="#" title="Facebook" data-tippy-placement="top">
							<i class="icon-brand-facebook-f"></i>
						</a>
					</li>
					<li>
						<a href="#" title="Twitter" data-tippy-placement="top">
							<i class="icon-brand-twitter"></i>
						</a>
					</li>
					<li>
						<a href="#" title="Google Plus" data-tippy-placement="top">
							<i class="icon-brand-google-plus-g"></i>
						</a>
					</li>
					<li>
						<a href="#" title="LinkedIn" data-tippy-placement="top">
							<i class="icon-brand-linkedin-in"></i>
						</a>
					</li>
				</ul>
				<div class="clearfix"></div>
			</div>
			<!-- Footer / End -->

		</div>
	</div>
	<!-- Dashboard Content / End -->

</div>
<!-- Dashboard Container / End -->

</div>
<!-- Wrapper / End -->


<!-- Scripts
================================================== -->
<script src="/js/jquery-3.4.1.min.js"></script>
<script src="/js/jquery-migrate-3.1.0.min.html"></script>
<script src="/js/mmenu.min.js"></script>
<script src="/js/tippy.all.min.js"></script>
<script src="/js/simplebar.min.js"></script>
<script src="/js/bootstrap-slider.min.js"></script>
<script src="/js/bootstrap-select.min.js"></script>
<script src="/js/snackbar.js"></script>
<script src="/js/clipboard.min.js"></script>
<script src="/js/counterup.min.js"></script>
<script src="/js/magnific-popup.min.js"></script>
<script src="/js/slick.min.js"></script>
<script src="/js/custom.js"></script>

<!-- Snackbar // documentation: https://www.polonel.com/snackbar/ -->
<script>
// Snackbar for user status switcher
$('#snackbar-user-status label').click(function() { 
	Snackbar.show({
		text: 'Your status has been changed!',
		pos: 'bottom-center',
		showAction: false,
		actionText: "Dismiss",
		duration: 3000,
		textColor: '#fff',
		backgroundColor: '#383838'
	}); 
}); 
</script>

<script>
	$('#logOut').on('click', () => {
		$.ajax({
			url: '/logout',
			method: 'GET',
			success: (data, status, xhr) => {
				window.location.href = '/';
			}
		});
	});
</script>

</body>
</html>